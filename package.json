{
  "name": "express-tsed-typeorm-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "pnpm run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "pnpm run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "pnpm run test:lint && pnpm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "pnpm run test:unit",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm_with_datasource": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/infra/db/data-source/PostgresDatasource.ts",
    "typeorm:migration:run": "pnpm run typeorm_with_datasource migration:run",
    "typeorm:migration:revert": "pnpm run typeorm_with_datasource migration:revert",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix"
  },
  "dependencies": {
    "@tsed/common": "^7.48.0",
    "@tsed/core": "^7.48.0",
    "@tsed/di": "^7.48.0",
    "@tsed/engines": "^7.48.0",
    "@tsed/exceptions": "^7.48.0",
    "@tsed/json-mapper": "^7.48.0",
    "@tsed/logger": "^6.6.3",
    "@tsed/logger-file": "^6.6.3",
    "@tsed/openspec": "^7.48.0",
    "@tsed/platform-cache": "^7.48.0",
    "@tsed/platform-exceptions": "^7.48.0",
    "@tsed/platform-express": "^7.48.0",
    "@tsed/platform-log-middleware": "^7.48.0",
    "@tsed/platform-middlewares": "^7.48.0",
    "@tsed/platform-params": "^7.48.0",
    "@tsed/platform-response-filter": "^7.48.0",
    "@tsed/platform-views": "^7.48.0",
    "@tsed/schema": "^7.48.0",
    "@tsed/swagger": "^7.48.0",
    "ajv": "^8.12.0",
    "barrelsby": "^2.8.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^4.0.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.6.4",
    "@tsed/cli-plugin-jest": "4.6.4",
    "@tsed/cli-plugin-typeorm": "4.6.4",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/method-override": "^0.0.35",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typeorm": "^0.3.17",
    "typescript": "^5.3.2"
  },
  "tsed": {
    "convention": "conv_default",
    "architecture": "feature",
    "packageManager": "pnpm"
  }
}